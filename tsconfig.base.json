{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@advantage-lite/cashiers-home": [
        "libs/cashiers/cashiers-home/src/index.ts"
      ],
      "@advantage-lite/cashiers-menu": [
        "libs/cashiers/cashiers-menu/src/index.ts"
      ],
      "@advantage-lite/cashiers-queues": [
        "libs/cashiers/cashiers-queues/src/index.ts"
      ],
      "@advantage-lite/cashiers-visits": [
        "libs/cashiers/cashiers-visits/src/index.ts"
      ],
      "@advantage-lite/consultation-home": [
        "libs/consultation/consultation-home/src/index.ts"
      ],
      "@advantage-lite/consultation-menu": [
        "libs/consultation/consultation-menu/src/index.ts"
      ],
      "@advantage-lite/consultation-queues": [
        "libs/consultation/consultation-queues/src/index.ts"
      ],
      "@advantage-lite/consultation-visits": [
        "libs/consultation/consultation-visits/src/index.ts"
      ],
      "@advantage-lite/login": ["libs/auth/login/src/index.ts"],
      "@advantage-lite/not-found": ["libs/auth/not-found/src/index.ts"],
      "@advantage-lite/patients-create": [
        "libs/patients/patients-create/src/index.ts"
      ],
      "@advantage-lite/patients-list": [
        "libs/patients/patients-list/src/index.ts"
      ],
      "@advantage-lite/select-app": ["libs/auth/select-app/src/index.ts"],
      "@advantage-lite/sil-menu": ["libs/shared/sil-menu/src/index.ts"],
      "@advantage-lite/sil-navbar": ["libs/shared/sil-navbar/src/index.ts"],
      "@sil-datatable/sil-datatable": [
        "libs/shared/sil-datatable/components/sil-datatable/src/index.ts"
      ],
      "cashier/Module": ["apps/cashier/src/app/remote-entry/entry.module.ts"],
      "consultation/Module": [
        "apps/consultation/src/app/remote-entry/entry.module.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
